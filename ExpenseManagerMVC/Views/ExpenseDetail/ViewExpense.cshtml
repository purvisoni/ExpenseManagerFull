@model List<ExpenseManager.ExpenseDetail>

<h2>Details of Expense</h2>
<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">StoreName</th>
                <th scope="col">ItemName</th>
                <th scope="col">Amount</th>
                <th scope="col">ExpenseDate</th>
                <th scope="col">Category</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr >
                <td>@Model[i].StoreName</td>
                <td>@Model[i].ItemName</td>
                <td>@Model[i].Amount</td>
                <td>@Model[i].ExpenseDate</td>
                <td>@Model[i].Category</td>
                
            </tr>
            }
        
        </tbody>
        
    </table>
    <div>
        <a asp-action="Form">New Expense</a>
    </div>

</div>